{
	"version": "0.6.1-alpha01",
	"description": "Even simpler real-time web for ASP.NET Core",

	"packOptions": {
		"repository": {
			"type": "git",
			"url": "https://github.com/buybackoff/SignalW"
		},
		"tags": [
			"SignalR SignalW websockets real-time messaging streaming"
		],
		"owners": [ "Victor Baybekov" ],
		"summary": "Even simpler real-time web for ASP.NET Core",
		"projectUrl": "https://github.com/buybackoff/SignalW"

	},
	"buildOptions": {
		"warningsAsErrors": false,
		"keyFile": "../../tools/Key.snk",
		"nowarn": [
			"CS1591"
		],
		"xmlDoc": true
	},

	"dependencies": {
		"Newtonsoft.Json": "9.0.1",
		"System.Buffers": "4.3.0"
	},

	"frameworks": {
		"netstandard1.3": {
			"dependencies": { "Microsoft.AspNetCore.WebSockets": "1.0.0" },
		},
		"net451": {
			"dependencies": {
				"System.Runtime": {
					"type": "build",
					"version": "4.1.0"
				}
			},

		}
	}
}
